{
  "name": "server",
  "version": "0.0.1",
  "description": "Software de remisión (Excepción de ausentismo) para la Uniminuto de Girardot, Colombia.",
  "main": "index.js",
  "scripts": {
    "kill": "fuser -k 8080/tcp &>/dev/null",
    "dev": "NODE_ENV=development nodemon --exec babel-node src/index.js",
    "build": "rm -r dist && babel src --ignore ./src/files/ -d dist --copy-files && cp -r ../client/build ./dist && mkdir ./dist/files",
    "start": "NODE_ENV=production node dist/index.js"
  },
  "keywords": [
    "uniminuto",
    "server",
    "ausentismo",
    "desercion"
  ],
  "author": "Andrés Mauricio Montoya Sánchez",
  "license": "MIT",
  "dependencies": {
    "apollo-server-express": "^1.3.6",
    "apollo-upload-server": "^7.0.0",
    "bcrypt": "^3.0.0",
    "compression": "^1.7.2",
    "convert-excel-to-json": "^1.3.0",
    "cors": "^2.8.4",
    "dataloader": "^1.4.0",
    "dotenv": "latest",
    "express": "^4.16.3",
    "graphql": "^0.13.2",
    "graphql-playground-middleware-express": "^1.7.0",
    "graphql-tools": "^3.0.2",
    "helmet": "^3.12.1",
    "json2xls": "^0.1.2",
    "jsonwebtoken": "^8.2.2",
    "lodash.pick": "^4.4.0",
    "merge-graphql-schemas": "^1.5.1",
    "moment": "^2.22.2",
    "mongoose": "^5.1.4",
    "nodemailer": "^4.6.7",
    "xlsx-chart": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "latest",
    "babel-plugin-transform-object-rest-spread": "latest",
    "babel-preset-env": "latest",
    "eslint": "4.19.1",
    "eslint-config-airbnb": "16.1.0",
    "eslint-config-airbnb-base": "12.1.0",
    "eslint-config-prettier": "2.10.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-prettier": "2.7.0",
    "eslint-plugin-react": "7.11.1",
    "nodemon": "latest",
    "prettier": "1.15.3"
  }
}
