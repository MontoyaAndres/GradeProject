{
  "extends": "airbnb-base",
  "plugins": ["import"],
  "env": {
    "es6": true,
    "node": true
  },
  "globals": {
    "describe": true,
    "test": true,
    "expect": true
  },
  "rules": {
    "strict": [2, "global"],
    "quotes": 0,
    "comma-dangle": 0,
    "function-paren-newline": 0,
    "arrow-parens": 0,
    "no-underscore-dangle": 0,
    "no-unused-vars": 0,
    "object-curly-newline": 0,
    "no-param-reassign": 0,
    "import/prefer-default-export": 0,
    "consistent-return": 0
  }
}
